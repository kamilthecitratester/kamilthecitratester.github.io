/* Plugin Config */
s.usePlugins=true
function s_doPlugins(s) {
	/* Add calls to plugins here */
	
	s.events = s.apl(s.events,"event1",",",1);
	s.eVar4 = s.getQueryParam('src_title_id');
	s.eVar9 = s.getValOnce(navigator.userAgent,"sc_uagent",0);
	/* 流入元 Identifierの計測。URLパラメータ名を設定してください */
	s.eVar14 = s.getQueryParam('*****');
	
	/* レンダリングモード */
	s.cWidth     = document.documentElement.clientWidth;
	s.renderMode = ""
	if (s.cWidth >= 320 && s.cWidth < 520) {
	    s.renderMode="smartphone";
	}
	if (s.cWidth >= 520 && s.cWidth < 800) {
	    s.renderMode="tablet";
	}
	if (s.cWidth >= 800) {
	    s.renderMode="desktop";
	}
	s.eVar31 = s.getValOnce(s.renderMode,"sc_rmode",30,"m");
	
	/* スクリーンサイズ */
	s.eVar32 = s.getValOnce(s.cWidth+'x'+document.documentElement.clientHeight,"sc_scsize",30,"m");

	/* スクリーンオリエンテーション */
	switch(window.orientation) {
	    case -90:
	    case 90:
	        s.eVar33="landscape";
	        break;
	    default:
	        s.eVar33="portrait";
	        break;
	}
}
s.doPlugins=s_doPlugins
/************************** PLUGINS SECTION *************************/
/* You may insert any plugins you wish to use here.                 */

